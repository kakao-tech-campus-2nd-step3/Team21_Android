<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CalendarHeaderTextAppearance" parent="TextAppearance.AppCompat.Large">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">25sp</item>
    </style>

    <style name="WhiteDialog" parent="ThemeOverlay.MaterialComponents.MaterialAlertDialog">
        <item name="android:background">#FFFFFF</item>
    </style>

    <style name="CustomDialogStyle" parent="@style/Theme.MaterialComponents.Dialog">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.72</item>
    </style>

    <style name="notificationToggleStyle" parent="Widget.AppCompat.CompoundButton.Switch"></style>
</resources>